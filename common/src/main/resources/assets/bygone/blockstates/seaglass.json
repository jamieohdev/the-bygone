{
  "multipart": [
    {   "apply": { "model": "bygone:block/seaglass" }},

    {   "when": { "up": "false" },
      "apply": { "model": "bygone:block/seaglass_up", "uvlock": false }
    },
    {   "when": { "down": "false" },
      "apply": { "model": "bygone:block/seaglass_down", "uvlock": false }
    },
    {   "when": { "east": "false" },
      "apply": { "model": "bygone:block/seaglass_right", "y" : 0, "uvlock": false }
    },
    {   "when": { "west": "false" },
      "apply": { "model": "bygone:block/seaglass_left", "y" : 0, "uvlock": false }
    },
    {   "when": { "north": "false" },
      "apply": { "model": "bygone:block/seaglass_left", "y" : 90, "uvlock": false }
    },
    {   "when": { "south": "false" },
      "apply": { "model": "bygone:block/seaglass_right", "y" : 90, "uvlock": false }
    }
  ]
}